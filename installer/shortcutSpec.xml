<shortcuts>
	<skipIfNotSupported/>
	<defaultCurrentUser/>
	<programGroup defaultName="SundaCoin" location="startMenu"/>
	<shortcut
		name="SundaCoin Server"
		target="$INSTALL_PATH\SundaCoin.exe"
		commandLine=""
		workingDirectory="$INSTALL_PATH"
        description="Start SundaCoin server"
		iconFile="$INSTALL_PATH\html\www\favicon.ico"
        iconIndex="0"
        initialState="normal"
        programGroup="yes"
		desktop="yes"
        applications="no"
        startMenu="no"
		startup="no"
		runAsAdministrator="true">
